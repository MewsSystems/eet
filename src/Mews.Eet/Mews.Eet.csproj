<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>7.3</LangVersion>
    <Authors>Jiří Helmich, Martin Šigut</Authors>
    <PackageProjectUrl>https://github.com/msigut/eet</PackageProjectUrl>
    <PackageLicenseUrl>https://raw.githubusercontent.com/msigut/eet/master/LICENSE</PackageLicenseUrl>
    <Product>Mews.Eet.NetStandard</Product>
    <Description>Client for EET - Czech fiscal machines.</Description>
    <PackageId>Mews.Eet.NetStandard</PackageId>
    <RepositoryUrl>https://github.com/msigut/eet</RepositoryUrl>
    <PackageTags>EET Fiscal Receipt</PackageTags>
    <Version>1.1.11</Version>
    <PackageReleaseNotes>Support .NET Standard 2.0</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageIconUrl>https://raw.githubusercontent.com/msigut/eet/master/receipt.png</PackageIconUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Security.Cryptography.Xml" Version="4.5.0" />
  </ItemGroup>

</Project>
